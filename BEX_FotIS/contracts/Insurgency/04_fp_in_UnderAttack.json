    "dialogueList": [
        {
            "dialogue": {
                "EncounterObjectGuid": "73df8d9c-a274-48fd-98c9-2bd0d7860e83"
            },
            "name": "Dialogue_MissionStart",
            "overrideDialogueBucketId": "",
            "dialogueContent": [
                {
                    "words": "We are down... My sensors are dead...",
                    "wordsColor": {
                        "r": 1,
                        "g": 1,
                        "b": 1,
                        "a": 1
                    },
                    "selectedCastDefId": "castDef_TeamPilot_Random_1",
                    "emote": "Default",
                    "audioName": "NONE",
                    "cameraFocusGuid": "",
                    "cameraDistance": "Far",
                    "cameraHeight": "Default",
                    "revealRadius": -1
                },
                {
                    "words": "Darius, what is the opposition?",
                    "wordsColor": {
                        "r": 1,
                        "g": 1,
                        "b": 1,
                        "a": 1
                    },
                    "selectedCastDefId": "castDef_Commander",
                    "emote": "Default",
                    "audioName": "NONE",
                    "cameraFocusGuid": "",
                    "cameraDistance": "Far",
                    "cameraHeight": "Default",
                    "revealRadius": -1
                },
                {
                    "words": "Medium and light 'Mechs. Seems like a mercenary outfit",
                    "wordsColor": {
                        "r": 1,
                        "g": 1,
                        "b": 1,
                        "a": 1
                    },
                    "selectedCastDefId": "castDef_DariusDefault",
                    "emote": "Default",
                    "audioName": "NONE",
                    "cameraFocusGuid": "",
                    "cameraDistance": "Far",
                    "cameraHeight": "Default",
                    "revealRadius": -1
                },
				{
                    "words": "Probably thought to get us with our guard down...",
                    "wordsColor": {
                        "r": 1,
                        "g": 1,
                        "b": 1,
                        "a": 1
                    },
                    "selectedCastDefId": "castDef_DariusDefault",
                    "emote": "Default",
                    "audioName": "NONE",
                    "cameraFocusGuid": "",
                    "cameraDistance": "Far",
                    "cameraHeight": "Default",
                    "revealRadius": -1
                },
				{
                    "words": "They didn't? I am mighty surprised boss...",
                    "wordsColor": {
                        "r": 1,
                        "g": 1,
                        "b": 1,
                        "a": 1
                    },
                    "selectedCastDefId": "castDef_TeamPilot_Random_2",
                    "emote": "Default",
                    "audioName": "NONE",
                    "cameraFocusGuid": "",
                    "cameraDistance": "Far",
                    "cameraHeight": "Default",
                    "revealRadius": -1
                },
				{
                    "words": "It is what it is; Group up and attack as team.",
                    "wordsColor": {
                        "r": 1,
                        "g": 1,
                        "b": 1,
                        "a": 1
                    },
                    "selectedCastDefId": "castDef_Commander",
                    "emote": "Default",
                    "audioName": "NONE",
                    "cameraFocusGuid": "",
                    "cameraDistance": "Far",
                    "cameraHeight": "Default",
                    "revealRadius": -1
                },
				{
                    "words": "Good luck everyone.",
                    "wordsColor": {
                        "r": 1,
                        "g": 1,
                        "b": 1,
                        "a": 1
                    },
                    "selectedCastDefId": "castDef_DariusDefault",
                    "emote": "Default",
                    "audioName": "NONE",
                    "cameraFocusGuid": "",
                    "cameraDistance": "Far",
                    "cameraHeight": "Default",
                    "revealRadius": -1
                }
				
            ]
        },
        {
            "dialogue": {
                "EncounterObjectGuid": "4011a4c3-cba2-4d22-b2b3-3b19a3297ab9"
            },
            "name": "Dialogue_MissionSuccess",
            "overrideDialogueBucketId": "",
            "dialogueContent": [
				
				{
                    "words": "I think it is all of them, Yang.",
                    "wordsColor": {
                        "r": 1,
                        "g": 1,
                        "b": 1,
                        "a": 1
                    },
                    "selectedCastDefId": "castDef_FPsold",
                    "emote": "Default",
                    "audioName": "NONE",
                    "cameraFocusGuid": "",
                    "cameraDistance": "Far",
                    "cameraHeight": "Default",
                    "revealRadius": -1
                },
			   {
                    "words": "Damn... I am getting too old for this...Good work, everyone.",
                    "wordsColor": {
                        "r": 1,
                        "g": 1,
                        "b": 1,
                        "a": 1
                    },
                    "selectedCastDefId": "castDef_YangDefault",
                    "emote": "Default",
                    "audioName": "NONE",
                    "cameraFocusGuid": "",
                    "cameraDistance": "Far",
                    "cameraHeight": "Default",
                    "revealRadius": -1
                },
				{
                    "words": "Thanks for saving us. You arrived right when we needed you. On behalf of everyone here, I can't thank you enough.",
                    "wordsColor": {
                        "r": 1,
                        "g": 1,
                        "b": 1,
                        "a": 1
                    },
                    "selectedCastDefId": "castDef_FarahDefault",
                    "emote": "Default",
                    "audioName": "NONE",
