    "dialogueList": [
        {
            "dialogue": {
                "EncounterObjectGuid": "73df8d9c-a274-48fd-98c9-2bd0d7860e83"
            },
            "name": "Dialogue_MissionStart",
            "overrideDialogueBucketId": "",
            "dialogueContent": [
                {
                    "words": "<color=#FAD02C>Something big landed behind behind the swing!\r\nI think that's them...Heading to the shelters with others.</color>",
                    "wordsColor": {
                        "r": 1,
                        "g": 1,
                        "b": 1,
                        "a": 1
                    },
                    "selectedCastDefId": "castDef_fp_daughter",
                    "emote": "Default",
                    "audioName": "NONE",
                    "cameraFocusGuid": "",
                    "cameraDistance": "Far",
                    "cameraHeight": "Default",
                    "revealRadius": -1
                },
                {
                    "words": "<color=#FF7F7F>Don't forget the mask! And extra filters! The storm is acting up. Take something warm...</color>",
                    "wordsColor": {
                        "r": 1,
                        "g": 1,
                        "b": 1,
                        "a": 1
                    },
                    "selectedCastDefId": "castDef_Krt01",
                    "emote": "Default",
                    "audioName": "NONE",
                    "cameraFocusGuid": "d9a28c08-abdb-42bb-a140-ad3927d1f8cf",
                    "cameraDistance": "Far",
                    "cameraHeight": "Default",
                    "revealRadius": -1
                },
                {
                    "words": "<color=#FAD02C>Don't worry Dad... Please, take care of yourself out there....\r\nLove you...</color>",
                    "wordsColor": {
                        "r": 1,
                        "g": 1,
                        "b": 1,
                        "a": 1
                    },
                    "selectedCastDefId": "castDef_fp_daughter",
                    "emote": "Default",
                    "audioName": "NONE",
                    "cameraFocusGuid": "",
                    "cameraDistance": "Far",
                    "cameraHeight": "Default",
                    "revealRadius": -1
                },
                {
                    "words": "That exchange revealed the position of one of the enemy lances.",
                    "wordsColor": {
                        "r": 1,
                        "g": 1,
                        "b": 1,
                        "a": 1
                    },
                    "selectedCastDefId": "castDef_DariusDefault",
                    "emote": "Default",
                    "audioName": "NONE",
                    "cameraFocusGuid": "76b654a6-4f2c-4a6f-86e6-d4cf868335fe",
                    "cameraDistance": "Far",
                    "cameraHeight": "Default",
                    "revealRadius": -1
                },
                {
                    "words": "One of the lances seem to have taken the position at the crossing here.",
                    "wordsColor": {
                        "r": 1,
                        "g": 1,
                        "b": 1,
                        "a": 1
                    },
                    "selectedCastDefId": "castDef_DariusDefault",
                    "emote": "Default",
                    "audioName": "NONE",
                    "cameraFocusGuid": "d9a28c08-abdb-42bb-a140-ad3927d1f8cf",
                    "cameraDistance": "Far",
                    "cameraHeight": "Default",
                    "revealRadius": -1
                },
                {
                    "words": "The other lance is probably in the hills overlooking the dam itself.",
                    "wordsColor": {
                        "r": 1,
                        "g": 1,
                        "b": 1,
                        "a": 1
                    },
                    "selectedCastDefId": "castDef_DariusDefault",
                    "emote": "Default",
                    "audioName": "NONE",
                    "cameraFocusGuid": "d3d75b1e-ace5-43e8-b037-79a2dd2b3d4e",
                    "cameraDistance": "Far",
                    "cameraHeight": "Default",
                    "revealRadius": -1
                },
                {
                    "words": "They might be just a militia, but there might be old veterans among them, serving out the last days of their service in the militia - so take care down there.",
                    "wordsColor": {
                        "r": 1,
                        "g": 1,
                        "b": 1,
                        "a": 1
                    },
                    "selectedCastDefId": "castDef_DariusDefault",
                    "emote": "Default",
                    "audioName": "NONE",
                    "cameraFocusGuid": "",
                    "cameraDistance": "Far",
                    "cameraHeight": "Default",
                    "revealRadius": -1
                }
            ]
        },
