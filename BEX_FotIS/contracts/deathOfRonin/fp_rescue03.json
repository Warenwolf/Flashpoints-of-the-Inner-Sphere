    "dialogueList" : [
        {
            "dialogue" : {
                "EncounterObjectGuid" : "73df8d9c-a274-48fd-98c9-2bd0d7860e83"
            },
            "name" : "Dialogue_MissionStart",
            "overrideDialogueBucketId" : "",
            "dialogueContent" : [
                {
                    "words" : "We're approaching the mineral prospecting site, Commander. Expect action soon.",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_DariusDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                },
                {
                    "words" : "Don't take this lightly. They must have seen us coming. Avoid open areas.",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_DariusDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                },
                {
                    "words" : "For your information, Commander, my unit has eliminated a demi-lance of 'Mechs. The remaining Ronin forces appear to have disengaged.",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_BakirMulleric",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                },
				{
                    "words" : "Be aware that the remaining Ronin forces might be heading back to the mining site. Good luck out there.",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_BakirMulleric",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                },
				{
                    "words" : "Once the battlefield is clear, we'll send in a team to get the kid out. Good luck down there, Commander",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_DariusDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                },
				{
                    "words" : "{MC.If.Company.HasTag.fp_ronin_farahwarning} Commander, after reviewing the video with the girl, I've determined that it's an old recording rather than a current broadcast. I would advise caution...",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_FarahDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                }
				
				
            ]
        },
        {
            "dialogue" : {
                "EncounterObjectGuid" : "4011a4c3-cba2-4d22-b2b3-3b19a3297ab9"
            },
            "name" : "Dialogue_MissionSuccess",
            "overrideDialogueBucketId" : "",
            "dialogueContent" : [
                {
                    "words" : "That was it, people. Sending rescue team down to find the kid. If she is still alive...",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_DariusDefault",
